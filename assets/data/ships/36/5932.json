ype":"point-images","id":"269","attributes":{"id":269,"point-id":5,"title":"","description":null,"width":300,"height":189,"mime":"image/jpeg","sort-order":1},"links":{"image-url":"https://static.mosturflot.ru/photo/5e/97/84/5e978429eae6a5462ebd301f842fea84.jpg","preview-url":"https://static.mosturflot.ru/photo/5e/97/84/5e978429eae6a5462ebd301f842fea84-p.jpg"}},{"type":"tour-excursions","id":"63012","attributes":{"id":63012,"excursion-id":1023,"name":"Обзорная автобусная экскурсия+реки и каналы(ИНТУР)","description":"<div>\r\n\t<a href=\"http://tour.mosturflot.ru/2015/10/27/spb01/\" target=\"_blank\">Обзорная автобусная экскурсия</a> познакомит Вас с самыми знаменитыми достопримечательностями города на Неве. Экскурсия на теплоходе &laquo;Реки и каналы Санкт-Петербурга&raquo;. Маршрут проходит по рекам: Мойка, Нева и Фонтанка. С борта теплохода Вы увидите Дворцовую набережную, Петропавловскую крепость, Михайловский дворец и многое другое&hellip;</div>\r\n","prices":null,"is-additional":false,"sort-order":6},"relationships":{"images":{"links":{"self":"https://api.mosturflot.ru/v3/rivercruises/tour-excursions/63012/relationships/images","related":"https://api.mosturflot.ru/v3/rivercruises/tour-excursions/63012/images"}}},"links":{"self":"https://api.mosturflot.ru/v3/rivercruises/tour-excursions/63012"}},{"type":"point-images","id":"294","attributes":{"id":294,"point-id":1,"title":"","description":null,"width":800,"height":365,"mime":"image/jpeg","sort-order":1},"links":{"image-url":"https://static.mosturflot.ru/photo/52/94/9c/52949cfe58be8be7b95e374b60901ef5.jpg","preview-url":"https://static.mosturflot.ru/photo/52/94/9c/52949cfe58be8be7b95e374b60901ef5-p.jpg"}}],"links":{"self":"https://api.mosturflot.ru/v3/rivercruises/tour-points?filter%5Btour-id%5D=5932&include=tour%2Ctour.ship%2Cexcursions%2Ctitle-image%2Ctour-rates&fields%5Btours%5D=ship-id%2Cdays%2Cstart%2Cfinish%2Croute%2Croute-note&fields%5Bships%5D=id%2Cname&per-page=10000&page=1"},"meta":{"total-count":8,"pag